if GetObjectName(Getmyhero())~="Amumu" then return end

require('Inspired')
require('Damagelib')

local WebLuaFile = "/Faqer/TES/master/Flamemumu.lua"
local WebVersion = "/Faqer/TES/master/Flamemumu.version"
local LocalLua = "Flamemumu.lua"
local LocalVersion = 1.0
AutoUpdate(WebLuaFile, WebVersion, LocalLua, LocalVersion)


PrintChat("Amumu loaded.") 
PrintChat("by Faqer")

local AmumuMenu = MenuConfig("Amumu","Amumu")

AmumuMenu:Menu("Combo","Combo")
AmumuMenu.Combo:Booblean("Q","Use Q", true)
AmumuMenu.Combo:Booblean("W","Use W", true)
AmumuMenu.Combo:Booblean("E","Use E", true)
AmumuMenu.Combo:Booblean("R","Use R", true)
AmumuMenu.Combo:Slider("Range", "Min range Q", 425,1, 750, 1)


AmumuMenu:Menu("Harass","Harass")
AmumuMenu.Harass:Booblean("Q","Use Q", true)
AmumuMenu.Harass:Booblean("W","Use W", true)
AmumuMenu.Harass:Booblean("E","Use E", true)
AmumuMenu.Harass:Slider("Mana","Min Mana %", 30, 1, 100,1)

AmumuMenu:Menu("Misc","Misc")
AmumuMenu.Misc:Boolean("Autolvl", "Auto level", true)
AmumuMenu.Misc:DropDown("Autolvltable", "Priority", 1, {"Q-W-E", "Q-E-W", "W-Q-E"})

